                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    @section Scripts {

        @Scripts.Render("~/assets2/js/tree.min.js")
        @Scripts.Render("~/assets2/js/ace-elements.min.js")
        @Scripts.Render("~/assets2/js/ace.min.js")
        @Scripts.Render("~/assets2/js/jquery.nestable.min.js")

        <script type="text/javascript">
            jQuery(function ($) {

                $('.dd').nestable();

                $('.dd-handle a').on('mousedown', function (e) {
                    e.stopPropagation();
                });

                $('[data-rel="tooltip"]').tooltip();

            });
        </script>
    }


